class Halfpyramid4
{
 public static void main(String args[])
 {
  int n=5;
  for (int i=0; i<n; i++)
        {
            // Print space in increasing order
            for (int j=n-1; j>i; j--) 
            { 
                System.out.print(" "); 
            } 
            // Print star in decreasing order 
            for (int j=0; j<=i; j++)
            {
                System.out.print("* ");
            }
            System.out.println();
        } 
 }
}

